{
  "name": "sanity-plugin-chatgpt-assist",
  "version": "1.1.3-alpha",
  "description": "Simple integration with ChatGPT API",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/bline/sanity-chatgpt-assist#readme",
  "bugs": {
    "url": "https://github.com/bline/sanity-chatgpt-assist/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bline/sanity-chatgpt-assist.git"
  },
  "license": "MIT",
  "author": "Scott Beck",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "node -r tsconfig-paths/register ./node_modules/.bin/plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@codemirror/closebrackets": "^0.19.2",
    "@codemirror/gutter": "^0.19.9",
    "@codemirror/matchbrackets": "^0.19.4",
    "@codemirror/state": "^6.5.1",
    "@codemirror/view": "^6.36.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@monaco-editor/react": "^4.7.0-rc.0",
    "@portabletext/react": "^3.2.0",
    "@portabletext/types": "^2.0.13",
    "@sanity/icons": "^3.5.6",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2.10.12",
    "@types/handlebars": "^4.0.40",
    "@types/mustache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@uiw/react-codemirror": "^4.23.7",
    "@xiechao/codemirror-lang-handlebars": "^1.0.4",
    "handlebars": "^4.7.8",
    "nanoid": "^5.0.9",
    "prism-react-editor": "^1.1.0",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@nx/vite": "^20.3.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@sanity/browserslist-config": "^1.0.5",
    "@sanity/eslint-config-no-v2-imports": "^0.0.1-studio-v3.3",
    "@sanity/pkg-utils": "^6.12.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-matchmedia-mock": "^1.1.0",
    "jest-styled-components": "^7.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanity": "^3.68.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": ">=4.7.4 <5.6.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^19",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "extends @sanity/browserslist-config"
  ],
  "resolutions": {
    "@codemirror/state": "6.5.1"
  }
}
